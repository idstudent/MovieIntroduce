<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.movieintroduce">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:name="com.MovieApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MovieIntroduce">
        <activity android:name="com.example.movieintroduce.presentation.view.MyLikeActivity"/>
        <activity android:name="com.example.movieintroduce.presentation.view.MovieDetailActivity" />
        <activity android:name="com.example.movieintroduce.presentation.view.MyLikeMovieDetailActivity"/>
        <activity android:name="com.example.movieintroduce.presentation.view.MovieIntroduceActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>